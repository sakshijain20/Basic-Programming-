#include<iostream>
using namespace std;
main(){
	int a[3][3]={1,2,3,2,1,4,0,5,5};
	int i,j,u=0,l=0;
		
	for(i=0;i<3;i++){
		for(j=0;j<3;j++){
			cout<<a[i][j];
		}
		printf("\n");
	}
	
	for(i=0;i<3;i++){
		for(j=0;j<3;j++){
			
			//upper
			if(i<j){
				u=u+a[i][j];
			}
			
			//lower
			else if(i>j){
				l=l+a[i][j];
			}
		}
	}
	
	cout<<"Lower triangular sum = "<<l<<endl;
	cout<<"Upper triangular sum = "<<u;
}
