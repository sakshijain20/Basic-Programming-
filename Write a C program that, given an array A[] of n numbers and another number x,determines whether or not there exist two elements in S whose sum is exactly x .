#include<iostream>
using namespace std;
void rotate(int a[],int n,int x){
	int i,flag=0;
	for(i=0;i<n;i++)
	{
		for(int j=i+1;j<n;j++)
		{
			if(a[i]+a[j]==x){
				flag=1;
				break;
			}
		}
	}
	if(flag)
		cout<<"Yes";
	else
		cout<<"No";
	
}

int main()
{
	
	int a[]={1,2,3,4,5,6};
	int n=sizeof(a);
	int x;
	cin>>x;
	rotate(a,n,x);
	return 0;
}
